<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.codevars.a20hospitalside">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PendingVerifications" />
        <activity android:name=".AllMembers" />
        <activity android:name=".BloodStock" />
        <activity android:name=".RequestBlood" />
        <activity android:name=".Team" />
        <activity android:name=".EmergencyRequest"></activity>
    </application>

</manifest>